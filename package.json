{
  "name": "vscode-extension-starter",
  "version": "0.0.1",
  "description": "vscode extension starter",
  "keywords": [
    "vscode extension starter",
    "vscode extension template"
  ],
  "license": "MIT",
  "author": "tangdaoyuan <1197633750@qq.com> (https://github.com/tangdaoyuan/)",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "ext:build": "cd extension && tsup src/extension.ts --external vscode",
    "ext:dev": "cd extension && tsup src/extension.ts --external vscode --watch src"
  },
  "resolutions": {
    "esbuild": "npm:esbuild-wasm"
  },
  "devDependencies": {
    "@suger-tdy/eslint-config": "^1.0.6",
    "@types/node": "^17.0.31",
    "eslint": "^8.14.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.4",
    "vitest": "^0.10.2"
  }
}
